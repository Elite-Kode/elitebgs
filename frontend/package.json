{
    "name": "elitebgs-frontend",
    "scripts": {
        "ng": "ng",
        "start": "run-s buildprod runprod",
        "startdev": "run-s removebuild preparedev",
        "build": "ng build",
        "lint": "ng lint",
        "removebuild": "rimraf dist",
        "preparedev": "run-p builddev rundev",
