{
  "name": "elitebgs-frontend",
  "private": true,
  "scripts": {
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "run-s lint:vue lint:node",
    "lint:vue": "vue-cli-service lint --no-fix -c .eslintrc.vue.js",
    "lint:node": "eslint \"{server,bin}/**/*.js\" -c .eslintrc.node.js",
    "lint:vue:fix": "vue-cli-service lint -c .eslintrc.vue.js",
    "lint:node:fix": "eslint \"{server,bin}/**/*.js\" --fix -c .eslintrc.node.js",
    "lint:fix": "run-s lint:vue:fix lint:node:fix",
    "builddev": "vue-cli-service build --watch",
    "buildprod": "vue-cli-service build",
    "dev": "vue-cli-service serve",
    "rundev": "cross-env NODE_ENV=development nodemon --inspect=localhost:9229 ./bin/start.js",
    "runprod": "cross-env PORT=4012 pm2 start process.json --env production",
    "start": "run-s buildprod runprod",
    "startdev": "run-p builddev rundev"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "consolidate": "^0.16.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "morgan": "^1.8.2",
    "rxjs": "^6.6.7",
    "vue-meta": "^2.4.0",
    "vue-rx": "^6.2.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@bugsnag/js": "^7.11.0",
    "@bugsnag/plugin-express": "^7.11.0",
    "@bugsnag/plugin-vue": "^7.11.0",
    "@highlightjs/vue-plugin": "^1.0.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-e2e-cypress": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.2",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.16.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.16.0",
    "highcharts": "^9.2.1",
    "highcharts-vue": "^1.4.0",
    "highlight.js": "^10.7.3",
    "mongoose": "^5.13.7",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.3",
    "pm2": "^5.1.1",
    "prettier": "^2.3.2",
    "sass": "^1.38.0",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.14",
    "vue-cli-plugin-vuetify": "^2.4.2",
    "vue-numeric": "^2.4.3",
    "vue-router": "^3.5.2",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.5.8",
    "vuetify-loader": "^1.4.3",
    "webpack-bugsnag-plugins": "^1.8.0"
  }
}
